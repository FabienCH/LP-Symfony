{% extends "mi06VitrineBundle:backoffice:layout.html.twig" %}
{% block content %}
    <h1>Categorie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ articleCategorie.id }}</td>
            </tr>
            <tr>
                <th>Intitule</th>
                <td>{{ articleCategorie.intitule }}</td>
            </tr>
        </tbody>
    </table>

    <p>
        <a href="{{ path('articlecategorie_index') }}">Retour</a><br/>
        <a href="{{ path('articlecategorie_edit', { 'id': articleCategorie.id }) }}">Edit</a><br/>
        {{ form_start(delete_form) }}
            <input class="btn btn-default" type="submit" value="Delete">
        {{ form_end(delete_form) }}
    </p>
{% endblock %}
